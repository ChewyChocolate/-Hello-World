#include <iostream>
using namespace std;

int main(){
    char letter[] {'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w' , 'x', 'y', 'z', '\0'};
    char word[] = {letter[2], letter[0], letter[19]};

    for(int i = 0; i < 3; i++){
    cout << word[i];
    }
    return 0;
}
